<?xml version="1.0" encoding="UTF-8"?>
<os id="com.arm.debug.os.uC3.std" version="5.23" xmlns='http://www.arm.com/os_extension'>
  <name>uC3_std.title</name>
  <implementer>Arm</implementer>
  <logo>uC3_standard_logo.png</logo>
  <description>uC3_std.desc</description>
  <provider>provider.py</provider>

  <parameter name="vfp-flag" description="uC3_std.param.vfp_desc" type="enum" default="disabled" help="uC3_std.param.vfp_help">
    <value   name="vfpv3_32" description="uC3_std.param.vfp_vfpv3_32"/>
    <value   name="vfpv3_16" description="uC3_std.param.vfp_vfpv3_16"/>
    <value   name="disabled" description="uC3_std.param.vfp_disabled"/>
  </parameter>

  <match arch="ARMv7A"/>
  <match arch="ARMv7R"/>
  <match arch="ARMv7AV"/>
</os>
