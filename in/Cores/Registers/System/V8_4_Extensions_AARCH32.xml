    <register access="RO" name="ID_ISAR6" size="8">
      <gui_name language="en">Instruction Set Attribute Register 6</gui_name>
      <description language="en">Provides information about the instruction sets implemented by the PE in AArch32 state...</description>
      <bitField enumerationId="ID_ISAR6_SPECRES" name="SPECRES">
        <gui_name language="en">SPECRES</gui_name>
        <description language="en">Speculation invalidation instruction support in AArch32 state.</description>
        <definition>[19:16]</definition>
      </bitField>
      <bitField enumerationId="ID_ISAR6_SB" name="SB">
        <gui_name language="en">SB</gui_name>
        <description language="en">SB instruction support in AArch32 state.</description>
        <definition>[15:12]</definition>
      </bitField>
      <bitField enumerationId="ID_ISAR6_FHM" name="FHM">
        <gui_name language="en">FHM</gui_name>
        <description language="en">Indicates whether VFMAL and VFMSL instructions are implemented.</description>
        <definition>[11:8]</definition>
      </bitField>
      <bitField enumerationId="ID_ISAR6_DP" name="DP">
        <gui_name language="en">DP</gui_name>
        <description language="en">Indicates the support for dot product instructions in AArch32 state.</description>
        <definition>[7:4]</definition>
      </bitField>
      <bitField enumerationId="ID_ISAR6_JSCVT" name="JSCVT">
        <gui_name language="en">JSCVT</gui_name>
        <description language="en">Indicates whether the Javascript conversion instruction is implemented in AArch32 state.</description>
        <definition>[3:0]</definition>
      </bitField>
    </register>
