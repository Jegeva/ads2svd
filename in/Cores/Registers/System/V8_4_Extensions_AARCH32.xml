    <tcf:enumItem description="CFPRCTX, DVPRCTX, and CPPRCTX instructions are implemented." name="CFPRCTX_DVPRCTX_and_CPPRCTX_instructions_are_implemented" number="1"/>
  </tcf:enumeration>
  <tcf:enumeration name="ID_ISAR6_SB">
