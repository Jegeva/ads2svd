<platform_data xmlns="http://www.arm.com/project_type" xmlns:peripheral="http://com.arm.targetconfigurationeditor" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" type="SNAPSHOT" xsi:schemaLocation="http://www.arm.com/project_type ../../../Schemas/platform_data-1.xsd">
    <project_type_list>
        <project_type type="BARE_METAL">
            <name language="en">View snapshot</name>
            <description language="en">This allows a snapshot connection.</description>
            <execution_environment id="COREDUMP">
                <name language="en">View snapshot</name>
                <description language="en">Connect to a snapshot image</description>
                <xi:include href="../../../Include/coredump_location.xml"/>
                <param default="CDB://snapshot.xml" id="config_file" type="string" visible="false"/>
                <param default="CDB://snapshot.py" id="dtsl_config_script" type="string" visible="false"/>
                <param default="Snapshot" id="dtsl_config" type="string" visible="false"/>
                <activity id="ICE_DEBUG" type="Debug">
                    <name language="en">View snapshot</name>
                    <xi:include href="../../../Include/coredump_activity_description.xml"/>
                    <xi:include href="../../../Include/coredump_connection_type.xml"/>
                    <core connection_id="__first__">
                        <core_definition>__dynamic__</core_definition>
                    </core>
                </activity>
            </execution_environment>
        </project_type>
        <project_type os_abstraction="LINUX_KERNEL" type="BARE_METAL">
            <name language="en">View snapshot with Linux Kernel and/or Device Driver Debug</name>
            <description language="en">View snapshot with Linux Kernel and/or Device Driver Debug</description>
            <execution_environment id="COREDUMP_LINUX_KERNEL_DEBUG">
                <name language="en">View snapshot with Linux Kernel Debug</name>
                <description language="en">Connect to a snapshot image with Linux Kernel debug extensions</description>
                <xi:include href="../../../Include/coredump_location.xml"/>
                <param default="CDB://snapshot.xml" id="config_file" type="string" visible="false"/>
                <param default="CDB://snapshot.py" id="dtsl_config_script" type="string" visible="false"/>
                <param default="Snapshot" id="dtsl_config" type="string" visible="false"/>
                <activity id="ICE_DEBUG" type="Debug">
                    <name language="en">View snapshot</name>
                    <xi:include href="../../../Include/coredump_activity_description.xml"/>
                    <xi:include href="../../../Include/coredump_connection_type.xml"/>
                    <core connection_id="__first__">
                        <core_definition>__dynamic__</core_definition>
                    </core>
                </activity>
            </execution_environment>
        </project_type>
    </project_type_list>
</platform_data>