    <project_type_list>
        <project_type type="BARE_METAL">
            <name language="en">Bare Metal Debug</name>
            <description language="en">This allows a bare-metal debug connection.</description>
            <execution_environment id="BARE_METAL">
                <name language="en">Bare Metal Debug</name>
                <description language="en">Connect to the simulation via the CADI interface</description>
                <param default="CDB://cadi_config.xml" id="config_file" type="string" visible="false"/>
                <!--The model_params param enables the model parameters field in the launch configuration. The parameters are passed to the setup_script.-->
                <param default="" id="model_params" type="string" visible="true">
                    <name>Model parameters</name>
                    <description>Model parameters</description>
                </param>
                <!--The script below is used to launch the model, if its executable is known. It can be used as a basis for more specific scripts.-->
                <setup_script>
                    <name>CDB://Scripts/rtsm_launcher.py</name>
                    <arguments>FVP_Base_Cortex-A57x4</arguments>
                </setup_script>
                <activity id="ICE_DEBUG" type="Debug">
                    <name language="en">Debug Cortex-A57_0</name>
                    <xi:include href="../../../Include/bare_metal_fvp_activity_description.xml"/>
                    <core connection_id="ARM_Cortex-A57_0">
                        <core_definition>Cortex-A57</core_definition>
                    </core>
                    <param default="CDB://dtsl_config_script.py" id="dtsl_config_script" type="string" visible="false"/>
                    <param default="DtslScript" id="dtsl_config" type="string" visible="false"/>
                    <param default="options.traceBuffer.traceCaptureDevice" id="dtsl_tracecapture_option" type="string" visible="false"/>
                </activity>
                <activity id="ICE_DEBUG" type="Debug">
                    <name language="en">Debug Cortex-A57_1</name>
