<flash_config
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.arm.com/flash_config"
    xsi:schemaLocation="http://www.arm.com/flash_config ../../../Schemas/flash_config.xsd">
    <devices>
        <!-- The main flash device -->
        <device name="TCFLASH">
            <programming_type type="FILE">
                <!-- Use the standard method for running Keil algorithms -->
                <method language="JYTHON" script="FDB://keil_flash.py" class="KeilFlash" method_config="TCFLASH"/>
                <!-- Target specific script to get target in a suitable state for programming -->
                <setup script="FDB://S6J3110_setup.py" method="setup"/>
            </programming_type>
        </device>
        <!-- The main flash device -->
        <device name="WORKFLASH">
            <programming_type type="FILE">
                <!-- Use the standard method for running Keil algorithms -->
                <method language="JYTHON" script="FDB://keil_flash.py" class="KeilFlash" method_config="WORKFLASH"/>
                <!-- Target specific script to get target in a suitable state for programming -->
                <setup script="FDB://S6J3110_setup.py" method="setup"/>
            </programming_type>
        </device>
    </devices>
    <method_configs>
        <!-- Parameters for programming the TCFLASH -->
        <method_config id="TCFLASH">
            <params>
                <!-- Programming algorithm binary to load to target -->
                <param name="algorithm" type="string" value="FDB://algorithms/S6J3110_TCFLASH.FLM"/>
                <!-- The core in the target to run the algorithm -->
                <param name="coreName" type="string" value="Cortex-R5"/>
                <!-- RAM location & size for algorithm code and write buffers -->
                <param name="ramAddress" type="integer" value="0x00000000"/>
                <param name="ramSize" type="integer" value="0x10000"/>
                <!-- Allow timeouts to be disabled -->
                <param name="disableTimeouts" type="string" value="false"/>
                <!-- Set to false to skip the verification stage -->
                <param name="verify" type="string" value="true"/>
            </params>
        </method_config>
        <!-- Parameters for programming the TCFLASH -->
        <method_config id="WORKFLASH">
            <params>
                <!-- Programming algorithm binary to load to target -->
                <param name="algorithm" type="string" value="FDB://algorithms/S6J3110_WORKFLASH.FLM"/>
                <!-- The core in the target to run the algorithm -->
                <param name="coreName" type="string" value="Cortex-R5"/>
                <!-- RAM location & size for algorithm code and write buffers -->
                <param name="ramAddress" type="integer" value="0x00000000"/>
                <param name="ramSize" type="integer" value="0x10000"/>
                <!-- Allow timeouts to be disabled -->
                <param name="disableTimeouts" type="string" value="false"/>
                <!-- Set to false to skip the verification stage -->
                <param name="verify" type="string" value="true"/>
            </params>
        </method_config>
    </method_configs>
</flash_config>
